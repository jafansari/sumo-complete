<?xml version="1.0" encoding="iso-8859-1"?>

<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.sf.net/xsd/sumoConfiguration.xsd">

    <input>
        <net-file value="" synonymes="n net"/>
        <route-files value="" synonymes="r routes"/>
        <additional-files value="" synonymes="a additional"/>
        <weight-files value="" synonymes="w weights"/>
        <measure value="" synonymes="m"/>
    </input>

    <output>
        <netstate-dump value="" synonymes="ndump netstate"/>
        <dump-empty-edges value="false"/>
        <summary value="" synonymes="emissions emissions-output"/>
        <tripinfo-output value="" synonymes="tripinfo"/>
        <vehroute-output value="" synonymes="vehroutes"/>
        <vehroute-output.exit-times value="false" synonymes="vehroutes.exit-times"/>
        <vehroute-output.last-route value="false" synonymes="vehroutes.last-route"/>
        <vehroute-output.sorted value="false" synonymes="vehroutes.sorted"/>
    </output>

    <time>
        <begin value="0" synonymes="b"/>
        <end value="-1" synonymes="e"/>
        <step-length value="1"/>
    </time>

    <processing>
        <route-steps value="200" synonymes="s"/>
        <no-internal-links value="false"/>
        <ignore-accidents value="false"/>
        <ignore-route-errors value="false"/>
        <too-many-vehicles value="-1"/>
        <incremental-dua-step value="-1"/>
        <incremental-dua-base value="10"/>
        <time-to-teleport value="300"/>
        <max-depart-delay value="-1"/>
        <sloppy-insert value="false"/>
        <lanechange.allow-swap value="false"/>
    </processing>

    <tls_defaults>
        <agent-tl.detector-len value="75"/>
        <agent-tl.learn-horizon value="3"/>
        <agent-tl.decision-horizon value="1"/>
        <agent-tl.min-diff value="0.1"/>
        <agent-tl.tcycle value="90"/>
        <actuated-tl.detector-pos value="100"/>
        <actuated-tl.max-gap value="3.1"/>
        <actuated-tl.detector-gap value="3"/>
        <actuated-tl.passing-time value="1.9"/>
    </tls_defaults>

    <report>
        <verbose value="false" synonymes="v"/>
        <print-options value="false" synonymes="p"/>
        <help value="false" synonymes="?"/>
        <version value="false" synonymes="V"/>
        <suppress-warnings value="false" synonymes="W"/>
        <log-file value="" synonymes="l"/>
        <no-duration-log value="false"/>
        <no-step-log value="false"/>
        <message-log value=""/>
        <error-log value=""/>
    </report>

    <routing>
        <device.routing.probability value="0"/>
        <device.routing.knownveh value=""/>
        <device.routing.deterministic value="false"/>
        <device.routing.period value="0"/>
        <device.routing.pre-period value="0"/>
        <device.routing.adaptation-weight value="0.5"/>
        <device.routing.adaptation-interval value="1"/>
        <device.routing.with-taz value="false"/>
    </routing>

    <emissions>
        <device.hbefa.probability value="0"/>
        <device.hbefa.knownveh value=""/>
        <device.hbefa.deterministic value="false"/>
    </emissions>

    <traci_server>
        <remote-port value="0"/>
    </traci_server>

    <random_number>
        <random value="false" synonymes="abs-rand"/>
        <seed value="23423" synonymes="srand"/>
    </random_number>

    <gui_only>
        <gui-settings-file value=""/>
    </gui_only>

</configuration>
