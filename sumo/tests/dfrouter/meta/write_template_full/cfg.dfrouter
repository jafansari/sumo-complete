<?xml version="1.0" encoding="iso-8859-1"?>

<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.sf.net/xsd/dfrouterConfiguration.xsd">

    <input>
        <net-file value="" synonymes="n net"/>
        <detector-files value="" synonymes="d detectors"/>
        <measure-files value="" synonymes="detector-flow-files detflows f"/>
    </input>

    <output>
        <routes-output value="" synonymes="o"/>
        <routes-for-all value="false"/>
        <detector-output value="" synonymes="detectors-output"/>
        <detectors-poi-output value=""/>
        <emitters-output value=""/>
        <emitters-poi-output value=""/>
        <variable-speed-sign-output value="" synonymes="speed-trigger-output"/>
        <end-reroute-output value=""/>
        <validation-output value=""/>
        <validation-output.add-sources value="false"/>
    </output>

    <processing>
        <guess-empty-flows value="false"/>
        <highway-mode value="false" synonymes="h"/>
        <ignore-invalid-detectors value="false"/>
        <revalidate-detectors value="false"/>
        <revalidate-routes value="false"/>
        <all-end-follower value="false"/>
        <keep-unfinished-routes value="false" synonymes="keep-unfound-ends"/>
        <keep-longer-routes value="false"/>
        <max-nodet-follower value="30"/>
        <emissions-only value="false"/>
        <disallowed-edges value=""/>
        <keep-turnarounds value="false"/>
        <min-distance value="-1" synonymes="min-dist"/>
        <time-factor value="60"/>
        <time-offset value="0"/>
        <time-step value="60"/>
        <write-calibrators value="false"/>
        <include-unused-routes value="false"/>
        <revalidate-flows value="false"/>
        <remove-empty-detectors value="false"/>
        <strict-sources value="false"/>
        <scale value="1"/>
    </processing>

    <time>
        <begin value="0" synonymes="b"/>
        <end value="86400" synonymes="e"/>
    </time>

    <report>
        <verbose value="false" synonymes="v"/>
        <print-options value="false" synonymes="p"/>
        <help value="false" synonymes="?"/>
        <version value="false" synonymes="V"/>
        <no-warnings value="false" synonymes="W suppress-warnings"/>
        <log value="" synonymes="l log-file"/>
        <message-log value=""/>
        <error-log value=""/>
        <report-empty-detectors value="false"/>
        <print-absolute-flows value="false"/>
    </report>

    <random_number>
        <random value="false" synonymes="abs-rand"/>
        <seed value="23423" synonymes="srand"/>
    </random_number>

</configuration>
