<configuration>

   <input>
      <!-- Load road network description from FILE -->
      <net-file></net-file>

      <!-- Load routes descriptions from FILE(s) -->
      <route-files></route-files>

      <!-- Load further descriptions from FILE(s) -->
      <additional-files></additional-files>

      <!-- Load weights from FILE -->
      <weight-files></weight-files>

   </input>

   <output>
      <!-- Save complete network states into FILE -->
      <netstate-dump></netstate-dump>

      <!-- Write also empty edges completely when dumping -->
      <dump-empty-edges>false</dump-empty-edges>

      <!-- Save aggregated vehicle emission inf. into FILE -->
      <emissions-output></emissions-output>

      <!-- Save single vehicle trip inf. into FILE -->
      <tripinfo-output></tripinfo-output>

      <!-- Save single vehicle route inf. into FILE -->
      <vehroute-output></vehroute-output>

      <!-- Write lane change information -->
      <lanechange-output></lanechange-output>

   </output>

   <time>
      <!-- Defines the begin time; The simulation starts at this time -->
      <begin>0</begin>

      <!-- Defines the end time; The simulation ends at this time -->
      <end>2147483647</end>

   </time>

   <processing>
      <!-- Load routes for the next INT steps ahead -->
      <route-steps>200</route-steps>

      <!-- Disable (junction) internal links -->
      <no-internal-links>false</no-internal-links>

      <!-- Quit (with an error) if an accident occures -->
      <quit-on-accident>false</quit-on-accident>

      <!-- Check whether accidents occure more deeply -->
      <check-accidents>false</check-accidents>

      <!-- Quit simulation if this number of vehicles is exceeded -->
      <too-many-vehicles>-1</too-many-vehicles>

      <!-- Perform the simulation as a step in incremental DUA -->
      <incremental-dua-step>-1</incremental-dua-step>

      <!-- Base value for incremental DUA -->
      <incremental-dua-base>10</incremental-dua-base>

      <!-- Specify how long a vehicle may wait until being teleported -->
      <time-to-teleport>300</time-to-teleport>

      <!--  -->
      <lanechange.min-sight></lanechange.min-sight>

      <!--  -->
      <lanechange.min-sight-edges>8</lanechange.min-sight-edges>

      <!-- Disable performance reports for individual simulation steps -->
      <no-duration-log>false</no-duration-log>

      <!-- Disable console output of current simulation step -->
      <no-step-log>false</no-step-log>

      <!-- How long vehicles wait for departure before being skipped -->
      <max-depart-delay>-1</max-depart-delay>

   </processing>

   <tls_defaults>
      <!--  -->
      <agent-tl.detector-len>75</agent-tl.detector-len>

      <!--  -->
      <agent-tl.learn-horizon>3</agent-tl.learn-horizon>

      <!--  -->
      <agent-tl.decision-horizon>1</agent-tl.decision-horizon>

      <!--  -->
      <agent-tl.min-diff>0.1</agent-tl.min-diff>

      <!--  -->
      <agent-tl.tcycle>90</agent-tl.tcycle>

      <!--  -->
      <actuated-tl.detector-pos>100</actuated-tl.detector-pos>

      <!--  -->
      <actuated-tl.max-gap>3.1</actuated-tl.max-gap>

      <!--  -->
      <actuated-tl.detector-gap>3</actuated-tl.detector-gap>

      <!--  -->
      <actuated-tl.passing-time>1.9</actuated-tl.passing-time>

   </tls_defaults>

   <report>
      <!-- Switches to verbose output -->
      <verbose>false</verbose>

      <!-- Prints option values before processing -->
      <print-options>false</print-options>

      <!-- Prints this screen -->
      <help>false</help>

      <!-- Disables output of warnings -->
      <suppress-warnings>false</suppress-warnings>

      <!-- Writes all messages to FILE -->
      <log-file></log-file>

      <!-- Writes all non-error messages to FILE -->
      <message-log></message-log>

      <!-- Writes all warnings and errors to FILE -->
      <error-log></error-log>

   </report>

   <traci_server>
      <!-- Enables TraCI Server if set -->
      <remote-port>0</remote-port>

      <!-- Value in 0..1 [default: 1] -->
      <penetration>1</penetration>

   </traci_server>

   <c2c>
      <!-- The probability for a vehicle to have c2c -->
      <device.c2x.probability>0</device.c2x.probability>

      <!-- Assign a device to named vehicles -->
      <device.c2x.knownveh></device.c2x.knownveh>

      <!-- The devices are set deterministic using a fraction of 1000 -->
      <device.c2x.deterministic>false</device.c2x.deterministic>

      <!-- The range of the c2c device -->
      <device.c2x.range>100</device.c2x.range>

      <!-- Duration of keeping messages -->
      <device.c2x.keep-duration>1800</device.c2x.keep-duration>

      <!-- Factor for adding messages -->
      <device.c2x.insert-info-factor>1.2</device.c2x.insert-info-factor>

      <!-- Save cluster information into FILE -->
      <c2x.cluster-info></c2x.cluster-info>

      <!-- Save 'connected' edges into FILE -->
      <c2x.edge-near-info></c2x.edge-near-info>

      <!--  -->
      <c2x.saved-info></c2x.saved-info>

      <!--  -->
      <c2x.saved-info-freq></c2x.saved-info-freq>

      <!-- Save transmitted information into FILE -->
      <c2x.transmitted-info></c2x.transmitted-info>

      <!-- Save names of connected vehicles into FILE -->
      <c2x.vehicle-in-range></c2x.vehicle-in-range>

   </c2c>

   <routing>
      <!-- The probability for a vehicle to have a routing device -->
      <device.routing.probability>0</device.routing.probability>

      <!-- Assign a device to named vehicles -->
      <device.routing.knownveh></device.routing.knownveh>

      <!-- The devices are set deterministic using a fraction of 1000 -->
      <device.routing.deterministic>false</device.routing.deterministic>

      <!-- The period with which the vehicle shall be rerouted -->
      <device.routing.period>0</device.routing.period>

      <!-- The weight of prior edge weights. -->
      <device.routing.adaptation-weight>0.5</device.routing.adaptation-weight>

   </routing>

   <emissions>
      <!-- The probability for a vehicle to have an emission logging device -->
      <device.hbefa.probability>0</device.hbefa.probability>

      <!-- Assign a device to named vehicles -->
      <device.hbefa.knownveh></device.hbefa.knownveh>

      <!-- The devices are set deterministic using a fraction of 1000 -->
      <device.hbefa.deterministic>false</device.hbefa.deterministic>

   </emissions>

   <random_number>
      <!-- Initialises the random number generator with the given value -->
      <srand>23423</srand>

      <!-- Initialises the random number generator with the current system time -->
      <abs-rand>false</abs-rand>

   </random_number>

   <gui_only>
      <!-- Load visualisation settings from FILE -->
      <gui-settings-file></gui-settings-file>

   </gui_only>

</configuration>
