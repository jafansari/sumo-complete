<?xml version="1.0" encoding="iso-8859-1"?>

<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.sf.net/xsd/sumoConfiguration.xsd">

    <input>
        <!-- Load road network description from FILE -->
        <net-file value="" synonymes="n net"/>

        <!-- Load routes descriptions from FILE(s) -->
        <route-files value="" synonymes="r routes"/>

        <!-- Load further descriptions from FILE(s) -->
        <additional-files value="" synonymes="a additional"/>

        <!-- Load edge/lane weights for online rerouting from FILE -->
        <weight-files value="" synonymes="w weights"/>

        <!-- Use the given attribute as weight when parsing weight files -->
        <weight-attribute value="traveltime" synonymes="m measure"/>

    </input>

    <output>
        <!-- Save complete network states into FILE -->
        <netstate-dump value="" synonymes="ndump netstate"/>

        <!-- Write also empty edges completely when dumping -->
        <netstate-dump.empty-edges value="false" synonymes="dump-empty-edges netstate.empty-edges"/>

        <!-- Save aggregated vehicle departure info into FILE -->
        <summary-output value="" synonymes="emissions emissions-output summary"/>

        <!-- Save single vehicle trip info into FILE -->
        <tripinfo-output value="" synonymes="tripinfo"/>

        <!-- Save single vehicle route info into FILE -->
        <vehroute-output value="" synonymes="vehroutes"/>

        <!-- Write the exit times for all edges -->
        <vehroute-output.exit-times value="false" synonymes="vehroutes.exit-times"/>

        <!-- Write the last route only -->
        <vehroute-output.last-route value="false" synonymes="vehroutes.last-route"/>

        <!-- Sorts the output by departure time -->
        <vehroute-output.sorted value="false" synonymes="vehroutes.sorted"/>

    </output>

    <time>
        <!-- Defines the begin time; The simulation starts at this time -->
        <begin value="0" synonymes="b"/>

        <!-- Defines the end time; The simulation ends at this time -->
        <end value="-1" synonymes="e"/>

        <!-- Defines the step duration -->
        <step-length value="1"/>

    </time>

    <processing>
        <!-- Load routes for the next INT steps ahead -->
        <route-steps value="200" synonymes="s"/>

        <!-- Disable (junction) internal links -->
        <no-internal-links value="false"/>

        <!-- Do not check whether accidents occure more deeply -->
        <ignore-accidents value="false"/>

        <!-- Do not check whether routes are connected -->
        <ignore-route-errors value="false"/>

        <!-- Quit simulation if this number of vehicles is exceeded -->
        <max-num-vehicles value="-1" synonymes="too-many-vehicles"/>

        <!-- Perform the simulation as a step in incremental DUA -->
        <incremental-dua-step value=""/>

        <!-- Base value for incremental DUA -->
        <incremental-dua-base value="10"/>

        <!-- Scale demand by the given factor (0..1) -->
        <scale value=""/>

        <!-- Specify how long a vehicle may wait until being teleported, defaults to 300, values < 1 disable teleporting -->
        <time-to-teleport value="300"/>

        <!-- How long vehicles wait for departure before being skipped, defaults to -1 which means vehicles are never skipped -->
        <max-depart-delay value="-1"/>

        <!-- Whether insertion on an edge shall not be repeated in same step once failed. -->
        <sloppy-insert value="false"/>

        <!-- Whether blocking vehicles trying to change lanes may be swapped. -->
        <lanechange.allow-swap value="false"/>

    </processing>

    <report>
        <!-- Switches to verbose output -->
        <verbose value="false" synonymes="v"/>

        <!-- Prints option values before processing -->
        <print-options value="false" synonymes="p"/>

        <!-- Prints this screen -->
        <help value="false" synonymes="?"/>

        <!-- Prints the current version -->
        <version value="false" synonymes="V"/>

        <!-- Disables output of warnings -->
        <no-warnings value="false" synonymes="W suppress-warnings"/>

        <!-- Writes all messages to FILE -->
        <log value="" synonymes="l log-file"/>

        <!-- Writes all non-error messages to FILE -->
        <message-log value=""/>

        <!-- Writes all warnings and errors to FILE -->
        <error-log value=""/>

        <!-- Disable performance reports for individual simulation steps -->
        <no-duration-log value="false"/>

        <!-- Disable console output of current simulation step -->
        <no-step-log value="false"/>

    </report>

    <routing>
        <!-- The probability for a vehicle to have a routing device -->
        <device.rerouting.probability value="0" synonymes="device.routing.probability"/>

        <!-- Assign a device to named vehicles -->
        <device.rerouting.explicit value="" synonymes="device.routing.knownveh"/>

        <!-- The devices are set deterministic using a fraction of 1000 -->
        <device.rerouting.deterministic value="false" synonymes="device.routing.deterministic"/>

        <!-- The period with which the vehicle shall be rerouted -->
        <device.rerouting.period value="0" synonymes="device.routing.period"/>

        <!-- The rerouting period before depart -->
        <device.rerouting.pre-period value="0" synonymes="device.routing.pre-period"/>

        <!-- The weight of prior edge weights. -->
        <device.rerouting.adaptation-weight value="0.5" synonymes="device.routing.adaptation-weight"/>

        <!-- The interval for updating the edge weights. -->
        <device.rerouting.adaptation-interval value="1" synonymes="device.routing.adaptation-interval"/>

        <!-- Use zones (districts) as routing end points -->
        <device.rerouting.with-taz value="false" synonymes="device.routing.with-taz"/>

    </routing>

    <emissions>
        <!-- The probability for a vehicle to have an emission logging device -->
        <device.hbefa.probability value="0"/>

        <!-- Assign a device to named vehicles -->
        <device.hbefa.explicit value="" synonymes="device.hbefa.knownveh"/>

        <!-- The devices are set deterministic using a fraction of 1000 -->
        <device.hbefa.deterministic value="false"/>

    </emissions>

    <traci_server>
        <!-- Enables TraCI Server if set -->
        <remote-port value="0"/>

    </traci_server>

    <random_number>
        <!-- Initialises the random number generator with the current system time -->
        <random value="false" synonymes="abs-rand"/>

        <!-- Initialises the random number generator with the given value -->
        <seed value="23423" synonymes="srand"/>

    </random_number>

    <gui_only>
        <!-- Load visualisation settings from FILE -->
        <gui-settings-file value=""/>

    </gui_only>

</configuration>
