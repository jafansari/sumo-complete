    <xsd:complexType name="processingType">
        <xsd:sequence>
            <xsd:element name="route-steps" type="route-stepsType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="no-internal-links" type="no-internal-linksType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="ignore-accidents" type="ignore-accidentsType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="too-many-vehicles" type="too-many-vehiclesType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="incremental-dua-step" type="incremental-dua-stepType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="incremental-dua-base" type="incremental-dua-baseType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="time-to-teleport" type="time-to-teleportType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="max-depart-delay" type="max-depart-delayType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="sloppy-emit" type="sloppy-emitType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="lanechange.allow-swap" type="lanechange.allow-swapType" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="route-stepsType">
        <xsd:attribute name="value" type="xsd:int" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="no-internal-linksType">
        <xsd:attribute name="value" type="xsd:boolean" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="ignore-accidentsType">
        <xsd:attribute name="value" type="xsd:boolean" use="required"/>
