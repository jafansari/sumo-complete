    </xsd:complexType>

    <xsd:complexType name="lanechange.allow-swapType">
        <xsd:attribute name="value" type="xsd:boolean" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="tls_defaultsType">
        <xsd:sequence>
            <xsd:element name="agent-tl.detector-len" type="agent-tl.detector-lenType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="agent-tl.learn-horizon" type="agent-tl.learn-horizonType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="agent-tl.decision-horizon" type="agent-tl.decision-horizonType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="agent-tl.min-diff" type="agent-tl.min-diffType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="agent-tl.tcycle" type="agent-tl.tcycleType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="actuated-tl.detector-pos" type="actuated-tl.detector-posType" minOccurs="0" maxOccurs="1"/>
