            <xsd:element name="device.routing.knownveh" type="device.routing.knownvehType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="device.routing.deterministic" type="device.routing.deterministicType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="device.routing.period" type="device.routing.periodType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="device.routing.pre-period" type="device.routing.pre-periodType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="device.routing.adaptation-weight" type="device.routing.adaptation-weightType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="device.routing.adaptation-interval" type="device.routing.adaptation-intervalType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="device.routing.with-taz" type="device.routing.with-tazType" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="device.routing.probabilityType">
        <xsd:attribute name="value" type="xsd:float" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="device.routing.knownvehType">
