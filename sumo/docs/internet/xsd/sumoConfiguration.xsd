            <xsd:element name="actuated-tl.passing-time" type="actuated-tl.passing-timeType" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="agent-tl.detector-lenType">
        <xsd:attribute name="value" type="xsd:float" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="agent-tl.learn-horizonType">
        <xsd:attribute name="value" type="xsd:int" use="required"/>
