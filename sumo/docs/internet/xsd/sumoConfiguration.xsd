            <xsd:element name="device.hbefa.probability" type="device.hbefa.probabilityType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="device.hbefa.knownveh" type="device.hbefa.knownvehType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="device.hbefa.deterministic" type="device.hbefa.deterministicType" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
