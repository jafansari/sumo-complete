            <xsd:element name="meso-edgelength" type="meso-edgelengthType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="meso-tauff" type="meso-tauffType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="meso-taufj" type="meso-taufjType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="meso-taujf" type="meso-taujfType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="meso-taujj" type="meso-taujjType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="meso-jam-threshold" type="meso-jam-thresholdType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="meso-multi-queue" type="meso-multi-queueType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="meso-junction-control" type="meso-junction-controlType" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="mesosimType">
        <xsd:attribute name="value" type="xsd:boolean" use="required"/>
