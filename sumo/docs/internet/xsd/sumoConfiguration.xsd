<?xml version="1.0" encoding="iso-8859-1"?>

<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <xsd:element name="configuration" type="configurationType"/>

    <xsd:complexType name="configurationType">
        <xsd:sequence>
            <xsd:element name="input" type="inputType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="output" type="outputType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="time" type="timeType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="processing" type="processingType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="tls_defaults" type="tls_defaultsType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="report" type="reportType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="routing" type="routingType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="emissions" type="emissionsType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="traci_server" type="traci_serverType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="mesoscopic" type="mesoscopicType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="random_number" type="random_numberType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="gui_only" type="gui_onlyType" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="inputType">
        <xsd:sequence>
            <xsd:element name="net-file" type="net-fileType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="route-files" type="route-filesType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="additional-files" type="additional-filesType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="weight-files" type="weight-filesType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="measure" type="measureType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="load-state" type="load-stateType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="load-state.offset" type="load-state.offsetType" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="net-fileType">
        <xsd:attribute name="value" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="route-filesType">
        <xsd:attribute name="value" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="additional-filesType">
        <xsd:attribute name="value" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="weight-filesType">
        <xsd:attribute name="value" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="measureType">
        <xsd:attribute name="value" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="load-stateType">
        <xsd:attribute name="value" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="load-state.offsetType">
        <xsd:attribute name="value" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="outputType">
        <xsd:sequence>
            <xsd:element name="netstate-dump" type="netstate-dumpType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="dump-empty-edges" type="dump-empty-edgesType" minOccurs="0" maxOccurs="1"/>
