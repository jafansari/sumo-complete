    <xsd:complexType name="device.routing.periodType">
        <xsd:attribute name="value" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="device.routing.pre-periodType">
        <xsd:attribute name="value" type="xsd:string" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="device.routing.adaptation-weightType">
        <xsd:attribute name="value" type="xsd:float" use="required"/>
